{
  "cleanupItems": [
    {
      "name": "Windows 临时文件夹",
      "path": "C:\\Windows\\Temp",
      "requireAdmin": true
    },
    {
      "name": "Windows 更新缓存",
      "path": "C:\\Windows\\SoftwareDistribution\\Download",
      "requireAdmin": true
    },
    {
      "name": "用户临时文件",
      "path": "C:\\Users\\%USERNAME%\\AppData\\Local\\Temp",
      "requireAdmin": false
    },
    {
      "name": "Edge 浏览器缓存",
      "path": "C:\\Users\\%USERNAME%\\AppData\\Local\\Microsoft\\Edge\\User Data\\Default\\Cache",
      "requireAdmin": false
    },
    {
      "name": "Chrome 浏览器缓存",
      "path": "C:\\Users\\%USERNAME%\\AppData\\Local\\Google\\Chrome\\User Data\\Default\\Cache",
      "requireAdmin": false
    },
    {
      "name": "Firefox 缓存",
      "path": "C:\\Users\\%USERNAME%\\AppData\\Local\\Mozilla\\Firefox\\Profiles",
      "requireAdmin": false
    },
    {
      "name": "应用崩溃转储文件",
      "path": "C:\\Users\\%USERNAME%\\AppData\\Local\\CrashDumps",
      "requireAdmin": false
    },
    {
      "name": "Visual Studio 缓存文件",
      "path": "C:\\Users\\%USERNAME%\\AppData\\Local\\Microsoft\\VisualStudio",
      "requireAdmin": false
    },
    {
      "name": "UWP 应用缓存",
      "path": "C:\\Users\\%USERNAME%\\AppData\\Local\\Packages",
      "requireAdmin": false
    }
  ]
}
